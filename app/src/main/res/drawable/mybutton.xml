<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#44C761" />
            <gradient android:angle="-90" android:startColor="#345953" android:endColor="#10DF1D"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#11E719" />
            <solid android:color="#1DBE5D"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#46DF1A" />
            <gradient android:angle="-90" android:startColor="#1ECF46" android:endColor="#33DA14" />
        </shape>
    </item>
</selector>